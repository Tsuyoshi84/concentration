{
  "name": "concentration",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --base-href \"https://tsuyoshi84.github.io/concentration/\" --no-progress",
    "test": "ng test",
    "test_without_watch": "ng test --watch=false --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "format": "biome format . --write",
    "format:check": "biome format .",
    "format:css": "prettier --write './src/**/*.css' --ignore-path .prettierignore",
    "lint-css": "stylelint '**/*.css' --ignore-path .gitignore",
    "lint-css:fix": "stylelint '**/*.css' --ignore-path .gitignore --fix",
    "check": "biome check . --apply",
    "check:fix": "biome check . --apply-unsafe",
    "e2e": "ng e2e",
    "deploy": "ng deploy --repo=https://github.com/Tsuyoshi84/concentration.git --base-href \"/concentration/\"  --message \"/[ci skip] Auto generated./\"",
    "ghpage": "angular-cli-ghpages --dir=dist/concentration/browser"
  },
  "private": true,
  "packageManager": "pnpm",
  "engines": {
    "node": ">=20.0.0",
    "npm": "use-pnpm",
    "yarn": "use-pnpm",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@angular/animations": "17.3.10",
    "@angular/common": "17.3.10",
    "@angular/compiler": "17.3.10",
    "@angular/core": "17.3.10",
    "@angular/forms": "17.3.10",
    "@angular/platform-browser": "17.3.10",
    "@angular/platform-browser-dynamic": "17.3.10",
    "@angular/router": "17.3.10",
    "lodash-es": "4.17.21",
    "open-props": "1.6.20",
    "postcss": "8.4.35",
    "reset-css": "5.0.2",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.2.2",
    "@angular/cli": "17.3.8",
    "@angular/compiler-cli": "17.3.10",
    "@biomejs/biome": "1.7.2",
    "@types/jasmine": "5.1.4",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.24",
    "angular-cli-ghpages": "1.0.7",
    "jasmine-core": "5.1.2",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.2.5",
    "stylelint": "16.2.1",
    "stylelint-config-pretty-order": "0.5.1",
    "stylelint-config-standard": "36.0.0",
    "stylelint-prettier": "5.0.0",
    "stylelint-use-logical-spec": "5.0.1",
    "ts-node": "10.9.2",
    "typescript": "~5.2.2"
  }
}
