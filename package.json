{
  "name": "concentration",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --base-href \"https://tsuyoshi84.github.io/concentration/\" --no-progress",
    "test": "ng test",
    "test_without_watch": "ng test --watch=false --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "ng lint",
    "fmt": "prettier --write './src/**/*.{ts,js,html}' --ignore-path .prettierignore",
    "fmt:check": "prettier --list-different './src/**/*.{ts,js,html}' --ignore-path .prettierignore ",
    "e2e": "ng e2e",
    "deploy": "ng deploy --repo=https://github.com/Tsuyoshi84/concentration.git --base-href \"/concentration/\" --message \"/[ci skip] Auto generated./\"",
    "ghpage": "angular-cli-ghpages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.4",
    "@angular/common": "12.2.4",
    "@angular/compiler": "12.2.4",
    "@angular/core": "12.2.4",
    "@angular/forms": "12.2.4",
    "@angular/platform-browser": "12.2.4",
    "@angular/platform-browser-dynamic": "12.2.4",
    "@angular/router": "12.2.4",
    "lodash-es": "4.17.21",
    "postcss": "8.3.8",
    "reset-css": "5.0.1",
    "rxjs": "7.3.0",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.7",
    "@angular/cli": "12.2.4",
    "@angular/compiler-cli": "12.2.4",
    "@types/jasmine": "3.9.1",
    "@types/lodash-es": "4.17.5",
    "@types/node": "14.17.20",
    "angular-cli-ghpages": "1.0.0-rc.2",
    "codelyzer": "6.0.2",
    "coveralls": "3.1.1",
    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "prettier": "2.4.1",
    "protractor": "7.0.0",
    "ts-node": "10.2.1",
    "tslint": "6.1.3",
    "typescript": "~4.2.4"
  }
}
