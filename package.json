{
  "name": "concentration",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --base-href \"https://tsuyoshi84.github.io/concentration/\" --no-progress",
    "test": "ng test",
    "test_without_watch": "ng test --watch=false --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "ng lint",
    "fmt": "prettier --write './src/**/*.{ts,js,html}' --ignore-path .prettierignore",
    "fmt:check": "prettier --list-different './src/**/*.{ts,js,html}' --ignore-path .prettierignore ",
    "e2e": "ng e2e",
    "deploy": "ng deploy --repo=https://github.com/Tsuyoshi84/concentration.git --base-href \"/concentration/\" --message \"/[ci skip] Auto generated./\"",
    "ghpage": "angular-cli-ghpages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.11",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/forms": "13.3.11",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "lodash-es": "4.17.21",
    "postcss": "8.4.14",
    "reset-css": "5.0.1",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.9",
    "@angular/cli": "13.3.9",
    "@angular/compiler-cli": "13.3.11",
    "@types/jasmine": "3.10.6",
    "@types/lodash-es": "4.17.6",
    "@types/node": "16.11.42",
    "angular-cli-ghpages": "1.0.0",
    "codelyzer": "6.0.2",
    "coveralls": "3.1.1",
    "jasmine-core": "4.2.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "prettier": "2.7.1",
    "protractor": "7.0.0",
    "ts-node": "10.8.1",
    "tslint": "6.1.3",
    "typescript": "~4.4.4"
  }
}
